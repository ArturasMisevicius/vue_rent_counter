{
  "enabled": false,
  "name": "Laravel Expert Code Generator",
  "description": "Generates/assesses Laravel code with project conventions (Laravel 12, Filament v4 admin, strict types, PSR-12, SOLID).",
  "version": "2.0",
  "when": {
    "type": "fileEdited",
    "patterns": [
      "app/**/*.php"
    ]
  },
  "then": {
    "type": "askAgent",
    "prompt": ""
  },
  "workspaceFolderName": "laravel",
  "shortName": "laravel-expert-assistant"
}